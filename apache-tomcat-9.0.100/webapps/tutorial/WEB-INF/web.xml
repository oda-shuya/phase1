<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
    
    <servlet>
        <servlet-name>CheckLogin</servlet-name>
        <servlet-class>CheckLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckLogin</servlet-name>
        <url-pattern>/checkLogin</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CheckRegister</servlet-name>
        <servlet-class>CheckRegister</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckRegister</servlet-name>
        <url-pattern>/checkRegister</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DataRegister</servlet-name>
        <servlet-class>DataRegister</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DataRegister</servlet-name>
        <url-pattern>/dataRegister</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>Login.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>LoginJSP</servlet-name>
        <jsp-file>/Login.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginJSP</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegisterJSP</servlet-name>
        <jsp-file>/Register.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterJSP</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PrintRegisterJSP</servlet-name>
        <jsp-file>/PrintRegister.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>PrintRegisterJSP</servlet-name>
        <url-pattern>/printRegister</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DoneLoginJSP</servlet-name>
        <jsp-file>/DoneLogin.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>DoneLoginJSP</servlet-name>
        <url-pattern>/doneLogin</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DoneRegisterJSP</servlet-name>
        <jsp-file>/DoneRegister.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>DoneRegisterJSP</servlet-name>
        <url-pattern>/doneRegister</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>testJSP</servlet-name>
        <jsp-file>/test.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>testJSP</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>

</web-app>
